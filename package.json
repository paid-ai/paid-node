{
    "name": "@paid-ai/paid-node",
    "version": "0.3.0-alpha3",
    "private": false,
    "repository": "https://github.com/paid-ai/paid-node",
    "type": "commonjs",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/cjs/index.d.ts",
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            },
            "default": "./dist/cjs/index.js"
        },
        "./openai": {
            "types": "./dist/cjs/openai-wrapper/index.d.ts",
            "import": {
                "types": "./dist/esm/openai-wrapper/index.d.mts",
                "default": "./dist/esm/openai-wrapper/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/openai-wrapper/index.d.ts",
                "default": "./dist/cjs/openai-wrapper/index.js"
            },
            "default": "./dist/cjs/openai-wrapper/index.js"
        },
        "./anthropic": {
            "types": "./dist/cjs/anthropic-wrapper/index.d.ts",
            "import": {
                "types": "./dist/esm/anthropic-wrapper/index.d.mts",
                "default": "./dist/esm/anthropic-wrapper/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/anthropic-wrapper/index.d.ts",
                "default": "./dist/cjs/anthropic-wrapper/index.js"
            },
            "default": "./dist/cjs/anthropic-wrapper/index.js"
        },
        "./mistral": {
            "types": "./dist/cjs/mistral-wrapper/index.d.ts",
            "import": {
                "types": "./dist/esm/mistral-wrapper/index.d.mts",
                "default": "./dist/esm/mistral-wrapper/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/mistral-wrapper/index.d.ts",
                "default": "./dist/cjs/mistral-wrapper/index.js"
            },
            "default": "./dist/cjs/mistral-wrapper/index.js"
        },
        "./langchain": {
            "types": "./dist/cjs/langchain-wrapper/index.d.ts",
            "import": {
                "types": "./dist/esm/langchain-wrapper/index.d.mts",
                "default": "./dist/esm/langchain-wrapper/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/langchain-wrapper/index.d.ts",
                "default": "./dist/cjs/langchain-wrapper/index.js"
            },
            "default": "./dist/cjs/langchain-wrapper/index.js"
        },
        "./vercel": {
            "types": "./dist/cjs/vercel-wrapper/index.d.ts",
            "import": {
                "types": "./dist/esm/vercel-wrapper/index.d.mts",
                "default": "./dist/esm/vercel-wrapper/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/vercel-wrapper/index.d.ts",
                "default": "./dist/cjs/vercel-wrapper/index.js"
            },
            "default": "./dist/cjs/vercel-wrapper/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "reference.md"
    ],
    "scripts": {
        "format": "prettier . --write --ignore-unknown",
        "build": "yarn build:cjs && yarn build:esm",
        "build:cjs": "tsc --project ./tsconfig.cjs.json",
        "build:esm": "tsc --project ./tsconfig.esm.json && node scripts/rename-to-esm-files.js dist/esm",
        "test": "jest tests/unit --passWithNoTests",
        "test:wire": "jest tests/wire --passWithNoTests",
        "wire:test": "yarn test:wire"
    },
    "dependencies": {
        "url-join": "4.0.1",
        "form-data": "^4.0.0",
        "formdata-node": "^6.0.3",
        "node-fetch": "^2.7.0",
        "qs": "^6.13.1",
        "readable-stream": "^4.5.2",
        "js-base64": "3.7.7",
        "@opentelemetry/api": ">=1.9.0",
        "@opentelemetry/exporter-trace-otlp-http": ">=0.202.0",
        "@opentelemetry/sdk-node": ">=0.202.0",
        "winston": ">=3.0.0"
    },
    "peerDependencies": {
        "@ai-sdk/openai": ">=1.0.0",
        "@anthropic-ai/sdk": ">=0.56.0",
        "@langchain/core": ">=0.3.0",
        "@mistralai/mistralai": ">=1.0.0",
        "ai": ">=5.0.0",
        "openai": ">=5.0.0"
    },
    "peerDependenciesMeta": {
        "@ai-sdk/openai": {
            "optional": true
        },
        "@anthropic-ai/sdk": {
            "optional": true
        },
        "@langchain/core": {
            "optional": true
        },
        "@mistralai/mistralai": {
            "optional": true
        },
        "ai": {
            "optional": true
        },
        "openai": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/url-join": "4.0.1",
        "@types/qs": "^6.9.17",
        "@types/node-fetch": "^2.6.12",
        "@types/readable-stream": "^4.0.18",
        "webpack": "^5.97.1",
        "ts-loader": "^9.5.1",
        "jest": "^29.7.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.14",
        "ts-jest": "^29.3.4",
        "jest-environment-jsdom": "^29.7.0",
        "msw": "^2.8.4",
        "@types/node": "^18.19.70",
        "prettier": "^3.4.2",
        "typescript": "~5.7.2",
        "@ai-sdk/openai": ">=1.0.0",
        "@anthropic-ai/sdk": ">=0.56.0",
        "@langchain/core": ">=0.3.0",
        "@mistralai/mistralai": ">=1.0.0",
        "ai": ">=5.0.0",
        "openai": ">=5.0.0"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false
    },
    "packageManager": "yarn@1.22.22"
}
