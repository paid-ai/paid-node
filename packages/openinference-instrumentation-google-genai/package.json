{
    "name": "openinference-instrumentation-google-genai",
    "version": "0.1.0",
    "description": "OpenInference instrumentation for Google GenAI (@google/genai)",
    "private": true,
    "main": "dist/src/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/src/index.d.ts",
    "scripts": {
        "prepare": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
        "type:check": "tsc --noEmit"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/src/index.js"
        }
    },
    "dependencies": {
        "@arizeai/openinference-core": "^2.0.5",
        "@arizeai/openinference-semantic-conventions": "^2.1.7",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/core": "^1.25.1",
        "@opentelemetry/instrumentation": "^0.46.0"
    },
    "devDependencies": {
        "@google/genai": "^1.0.0",
        "@opentelemetry/exporter-trace-otlp-proto": "^0.50.0",
        "@opentelemetry/resources": "^1.25.1",
        "@opentelemetry/sdk-trace-base": "^1.25.1",
        "@opentelemetry/sdk-trace-node": "^1.25.1",
        "@opentelemetry/semantic-conventions": "^1.25.1",
        "typescript": "^5.0.0"
    }
}
